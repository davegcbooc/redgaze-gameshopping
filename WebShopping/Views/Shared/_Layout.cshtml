<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - RedGaze Games Shop</title>
    <meta charset="utf-8">

    <title>RedGaze Game Shop</title>


    <link rel="icon" type="image/png" href="~/Content//images/favicon.png">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700%7cOpen+Sans:400,700" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/Content//vendor/bootstrap/dist/css/bootstrap.min.css">
    <script defer src="~/Content//vendor/fontawesome-free/js/all.js"></script>
    <script defer src="~/Content//vendor/fontawesome-free/js/v4-shims.js"></script>
    <link rel="stylesheet" href="~/Content//vendor/ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content//vendor/flickity/dist/flickity.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content//vendor/photoswipe/dist/photoswipe.css">
    <link rel="stylesheet" type="text/css" href="~/Content//vendor/photoswipe/dist/default-skin/default-skin.css">
    <link rel="stylesheet" href="~/Content//vendor/bootstrap-slider/dist/css/bootstrap-slider.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content//vendor/summernote/dist/summernote-bs4.css">
    <link rel="stylesheet" href="~/Content//css/goodgames.css">
    <link rel="stylesheet" href="~/Content//css/custom.css">
    <script src="~/Content//vendor/jquery/dist/jquery.min.js"></script>

</head>

<body>

        @RenderBody()

        <div class="nk-gap-2"></div>

        <!--Footer-->
        <footer class="nk-footer">

            <div class="container">
                <div class="nk-gap-3"></div>
            </div>

            <div class="nk-copyright">
                <div class="container">
                    <div class="nk-copyright-left">
                        <a target="_blank" href="">&copy; @DateTime.Now.Year - RedGaze Games  Shop ASP.NET </a>
                    </div>

                </div>
            </div>
        </footer>
        <!--Footer -->


    <script src="~/Content//vendor/object-fit-images/dist/ofi.min.js"></script>

    <!-- GSAP -->
    <script src="~/Content//vendor/gsap/src/minified/TweenMax.min.js"></script>
    <script src="~/Content//vendor/gsap/src/minified/plugins/ScrollToPlugin.min.js"></script>

    <!-- Popper -->
    <script src="~/Content//vendor/popper.js/dist/umd/popper.min.js"></script>

    <!-- Bootstrap -->
    <script src="~/Content//vendor/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Sticky Kit -->
    <script src="~/Content//vendor/sticky-kit/dist/sticky-kit.min.js"></script>

    <!-- Jarallax -->
    <script src="~/Content//vendor/jarallax/dist/jarallax.min.js"></script>
    <script src="~/Content//vendor/jarallax/dist/jarallax-video.min.js"></script>

    <!-- imagesLoaded -->
    <script src="~/Content//vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>

    <!-- Flickity -->
    <script src="~/Content//vendor/flickity/dist/flickity.pkgd.min.js"></script>

    <!-- Photoswipe -->
    <script src="~/Content//vendor/photoswipe/dist/photoswipe.min.js"></script>
    <script src="~/Content//vendor/photoswipe/dist/photoswipe-ui-default.min.js"></script>

    <!-- Jquery Validation -->
    <script src="~/Content//vendor/jquery-validation/dist/jquery.validate.min.js"></script>

    <!-- Jquery Countdown + Moment -->
    <script src="~/Content//vendor/jquery-countdown/dist/jquery.countdown.min.js"></script>
    <script src="~/Content//vendor/moment/min/moment.min.js"></script>
    <script src="~/Content//vendor/moment-timezone/builds/moment-timezone-with-data.min.js"></script>

    <!-- Hammer.js -->
    <script src="~/Content//vendor/hammerjs/hammer.min.js"></script>

    <!-- NanoSroller -->
    <script src="~/Content//vendor/nanoscroller/bin/javascripts/jquery.nanoscroller.js"></script>

    <!-- SoundManager2 -->
    <script src="~/Content//vendor/soundmanager2/script/soundmanager2-nodebug-jsmin.js"></script>

    <!-- Seiyria Bootstrap Slider -->
    <script src="~/Content//vendor/bootstrap-slider/dist/bootstrap-slider.min.js"></script>

    <!-- Summernote -->
    <script src="~/Content//vendor/summernote/dist/summernote-bs4.min.js"></script>

    <!-- nK Share -->
    <script src="~/Content//plugins/nk-share/nk-share.js"></script>

    <!-- GoodGames -->
    <script src="~/Content//js/goodgames.min.js"></script>
    <script src="~/Content//js/goodgames-init.js"></script>
    <!-- END: Scripts -->

</body>
</html>
